public class A{
	{
		System.out.println("From IIB");
	}
	public static void main(String[] args) {
	
	}
}
/// NO output because object not created

//Example:2
package p1;
public class A{
	{
		System.out.println("From IIB");
	}
	public static void main(String[] args) {
	 A a1 = new A();/// ..Object is created
	}
}
///Example :3

package p1;
public class A{
	A(){
		System.out.println("From constructor");
	}
	{
		System.out.println("From IIB");
	}
	public static void main(String[] args) {
	 A a1 = new A();
	 
	}
}
/// Example :4
package p1;
public class A{
	{
		System.out.println("From IIB");
	}
	A(){
		System.out.println("From constructor");
	}
	
	public static void main(String[] args) {
		System.out.println("start main");
	 A a1 = new A();
	 System.out.println("End main");
	}
}

//// Example :5
package p1;
public class A{
	
	A(){
		System.out.println("From constructor");
	}
	{
		System.out.println("From IBB-1");
	}
	{
		System.out.println("From IIB-2");
	}
	
	public static void main(String[] args) {
	 A a1 = new A();
	 
	}
}
/// Now initialization the variable values
package p1;
public class A{
	
	int i;
	{
		i =20;
		System.out.println(i);
	}
	public static void main(String[] args) {
		A a1 = new A();
	}
}